#: import utils kivy.utils
#: import Factory kivy.factory.Factory

WindowManager:
    MainWindow:
    CreateExamWindow:
    QuestionnaireWindow:
    InputQuestionWindow:
    PreviewQWindow:
    PreviewAWindow:
    CheckExamWindow:
    NewSetWindow:
    AnswerKeyWindow:
    StudentWindow:

<sp@SpinnerOption>
    background_normal: ''
    background_color: 1,1,1,1
    color: (0,0,0,1)
    font_size: self.height/3

<RoundedBackGoButton@Button>:
    font_size: (self.width/50)*(self.height/5)
    color: (1,1,1,1)
    size_hint: (.4,.9)
    font_name: 'century gothic'
    background_color:(0,0,0,0)
    background_normal:''
    canvas.before:
        Color:
            rgba: utils.get_color_from_hex('006b38ff')
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [18]
<Font>:
    font_name: 'century gothic'

<Label>:
    halign: 'center'
    valign: 'middle'

<LabelAtTop@Label>
    font_size: (self.width/50)*(self.height/13)
    color: (1,1,1,1)
    font_name: 'century gothic'

<CameraClick>:
    id: popop
    name: "camerawin"
    size_hint:.8,.7
    auto_dismiss:False
    title:""
    separator_height: 0
    color: (0,0,0,1)
    background:''
    BoxLayout:
        orientation: 'vertical'
        Camera:
            id: camera
            size: (700,400)
            resolution: (1500, 2000)
            play: True
            index: 1
        Button:
            text: 'Capture'
            size_hint_y: None
            height: '48dp'
            on_press:
                camera.export_to_png('/img/OMR_Sheet.png')
                root.save_ans()
                popop.dismiss()


<MyPopUp>:
    id:pop
    size_hint:.7,.4
    auto_dismiss:False
    title:""
    separator_height: 0
    color: (0,0,0,1)
    background:''
    FloatLayout:
        id: pop_up
        Label:
            size_hint:1,.2
            text:"Name of the Paper"
            font_size: (self.width/30)*(self.height/15)
            color: (0,0,0,1)
            pos_hint:{"center_x":.5,"center_y":.8}
        TextInput:
            id: std_name
            hint_text:"shet"
            size_hint:1,.2
            halign: 'center'
            valign: 'middle'
            cursor_color: (0,0,0,1)
            pos_hint:{"center_x":.5,"center_y":.5}
            canvas.after:
                Color:
                    rgba: utils.get_color_from_hex('006b38ff')
                Line:    # --- adds a border --- #
                    width: .5
                    rectangle: self.x, self.y, self.width, self.height
        Button:
            text:"Continue"
            size_hint:1,.2
            on_press: pop.dismiss()
            pos_hint:{"center_x":.5,"center_y":.1}
            background_color:(0,0,0,0)
            background_normal:''
            canvas.before:
                Color:
                    rgba: utils.get_color_from_hex('006b38ff')
                Rectangle:
                    size: self.size
                    pos: self.pos
            on_press:
                root.std_save(std_name.text)
                pop.dismiss()
            on_release:
                root.btnn()

        Button: #Exit Button
            background_normal: 'exit.png'
            background_down: 'exit.png'
            size_hint:.3,.3
            pos_hint:{"center_x":.94,"center_y":1.03}
            on_press: pop.dismiss()



